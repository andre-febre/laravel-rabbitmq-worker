{
    "name": "edipoelwes/laravel-rabbitmq-worker",
    "description": "This project is a Laravel package that facilitates the execution of RabbitMQ worker processes. It simplifies the configuration, management, and monitoring of worker processes, allowing you to easily integrate RabbitMQ into your Laravel application to handle asynchronous tasks efficiently and scalably.",
    "keywords": [
        "edipoelwes",
        "laravel-rabbitmq-worker"
    ],
    "homepage": "https://github.com/edipoelwes/laravel-rabbitmq-worker",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Edipoelwes\\LaravelRabbitmqWorker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Edipo Elwes",
            "email": "edipoelwes2@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "php-amqplib/php-amqplib": "^3.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edipoelwes\\LaravelRabbitmqWorker\\CommandServiceProvider"
            ]
        }
    }
}
